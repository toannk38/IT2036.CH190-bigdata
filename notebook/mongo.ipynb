    "res = client.vietnam_stock_ai.price_history.aggregate([\n",
    "    {\"$group\": {\"_id\": \"$symbol\", \"count\": {\"$sum\": 1}}}\n",
    "])\n",
    "res = sorted(list(res), key=lambda x: x['count'])\n",
    "res"
   ]
