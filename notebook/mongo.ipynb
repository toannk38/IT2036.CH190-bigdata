    "from pymongo import MongoClient\n",
    "import time\n",
    "client = MongoClient()\n",
    "collection = client.vietnam_stock_ai.price_history\n",
    "while True:\n",
    "    count = collection.count_documents({\"timestamp\": {\"$not\": {\"$type\": \"double\"}}})\n",
    "    print(f\"Number of records where timestamp is not float: {count}\", end=\"\\r\")\n",
    "    time.sleep(3)\n"
   ]
